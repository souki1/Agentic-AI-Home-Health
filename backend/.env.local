ENV_NAME=local

# PostgreSQL only. Create DB first: createdb -U postgres health_analytics  (or via pgAdmin)
DATABASE_URL=postgresql://postgres:root@localhost:5433/health_analytics
DB_POOL_SIZE=5
DB_POOL_RECYCLE_SECONDS=300

# Auth (used by config.py)
SECRET_KEY=root
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# Server bind (used by config.py)
HOST=0.0.0.0
PORT=8000

# CORS: comma-separated allowed origins (used by config.py)
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173,http://localhost:3000,http://127.0.0.1:3000

# LLM / RAG (local uses Ollama)
LLM_PROVIDER=ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2

# Other
BACKEND_BASE_URL=http://localhost:8000
LOG_LEVEL=INFO
